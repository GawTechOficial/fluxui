  "scripts": {
    "build": "rimraf dist && tsx ./scripts/build.ts",
    "dev": "tsx watch ./scripts/build.ts"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/tokens.css"
  ],
  "devDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
